   "source": [
    "import pybithumb\n",
    "\n",
    "df = pybithumb.get_ohlcv(\"BTC\")\n",
    "print(df)\n",
    "yesterday = df.iloc[-2]\n",
    "\n",
    "today_open = yesterday['close']\n",
    "yesterday_high = yesterday['high']\n",
    "yesterday_low = yesterday['low']\n",
    "target = today_open + (yesterday_high - yesterday_low)\n",
    "print(target)"
   ]
